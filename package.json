{
  "name": "angular-route-xxl",
  "version": "1.5.0",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "https://github.com/scaljeri/angular-route-xxl.git",
  "author": "lucascalje <lucas@calje.eu>",
  "license": "MIT",
  "keywords": [
    "angular",
    "router",
    "resolve",
    "parameter",
    "data",
    "decorator"
  ],
  "scripts": {
    "build": "rm -rf bundle && ./node_modules/.bin/tsc && ./node_modules/.bin/tsc -d --lib 'es2015, dom' ./index.ts",
    "prepublish": "yarn build",
    "test": "./node_modules/.bin/nyc mocha",
    "stryker": "./node_modules/.bin/stryker run",
    "coveralls": "./node_modules/.bin/coveralls < coverage/lcov.info",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json 'src/*.ts'"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.47",
    "@types/sinon-chai": "2.7.29",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "mocha": "5.0.0",
    "nyc": "11.4.1",
    "rxjs": "5.5.6",
    "rxjs-tslint-rules": "3.6.0",
    "sinon": "4.2.1",
    "sinon-chai": "2.14.0",
    "stryker": "0.18.1",
    "stryker-api": "0.12.0",
    "stryker-cli": "0.1.3",
    "stryker-html-reporter": "0.11.5",
    "stryker-mocha-framework": "0.8.1",
    "stryker-mocha-runner": "0.10.3",
    "stryker-typescript": "0.8.3",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.0"
  },
  "dependencies": {}
}
